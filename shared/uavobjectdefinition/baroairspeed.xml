<?xml version="1.0"?>
<xml>
  <object name="BaroAirspeed" singleinstance="true" settings="false">
    <meta>
      <description>The raw data from the dynamic pressure sensor with pressure, temperature and airspeed.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="BaroConnected" units="" type="enum" elements="1" options="False,True"/>
      <data name="SensorValue" units="raw" type="uint16" elements="1"/>
      <data name="CalibratedAirspeed" units="m/s" type="float" elements="1"/>
      <data name="GPSAirspeed" units="m/s" type="float" elements="1"/>
      <data name="TrueAirspeed" units="m/s" type="float" elements="1"/>
    </data>
  </object>
</xml>
