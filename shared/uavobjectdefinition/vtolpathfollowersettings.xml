<?xml version="1.0"?>
<xml>
  <object name="VtolPathFollowerSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the @ref VtolPathFollowerModule</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="GuidanceMode" units="" type="enum" elements="1" options="DUAL_LOOP,VELOCITY_CONTROL" defaultvalue="DUAL_LOOP"/>
      <data name="HorizontalVelMax" units="m/s" type="uint16" elements="1" defaultvalue="10"/>
      <data name="VerticalVelMax" units="m/s" type="uint16" elements="1" defaultvalue="1"/>
      <data name="HorizontalPosPI" units="(m/s)/m" type="float" elementnames="Kp,Ki,ILimit" defaultvalue="1,0,0"/>
      <data name="HorizontalVelPID" units="deg/(m/s)" type="float" elementnames="Kp,Ki,Kd,ILimit" defaultvalue="5,0,1,0"/>
      <data name="VerticalPosPI" units="" type="float" elementnames="Kp,Ki,ILimit" defaultvalue="0.1,0.001,200"/>
      <data name="VerticalVelPID" units="" type="float" elementnames="Kp,Ki,Kd,ILimit" defaultvalue="0.1,0,0,0"/>
      <data name="VelocityFeedforward" units="deg/(m/s)" type="float" elements="1" defaultvalue="0"/>
      <data name="ThrottleControl" units="" type="enum" elements="1" options="FALSE,TRUE" defaultvalue="FALSE"/>
      <data name="VelocitySource" units="" type="enum" elements="1" options="EKF,NEDVEL,GPSPOS" defaultvalue="EKF"/>
      <data name="PositionSource" units="" type="enum" elements="1" options="EKF,GPSPOS" defaultvalue="EKF"/>
      <data name="MaxRollPitch" units="deg" type="float" elements="1" defaultvalue="20"/>
      <data name="UpdatePeriod" units="ms" type="int32" elements="1" defaultvalue="100"/>
    </data>
  </object>
</xml>
