<?xml version="1.0"?>
<xml>
  <object name="GroundTruth" singleinstance="true" settings="false">
    <meta>
      <description>Ground truth data output by a simulator.</description>
      <access gcs="readwrite" flight="readonly"/>
      <telemetrygcs acked="false" updatemode="periodic" period="50000"/>
      <telemetryflight acked="false" updatemode="manual" period="50000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="AccelerationXYZ" units="m/s^2" type="float" elements="3"/>
      <data name="PositionNED" units="m" type="float" elements="3"/>
      <data name="VelocityNED" units="m/s" type="float" elements="3"/>
      <data name="RPY" units="deg" type="float" elements="3"/>
      <data name="AngularRates" units="deg/s" type="float" elements="3"/>
      <data name="TrueAirspeed" units="m/s" type="float" elements="1"/>
      <data name="CalibratedAirspeed" units="m/s" type="float" elements="1"/>
      <data name="AngleOfAttack" units="deg" type="float" elements="1"/>
      <data name="AngleOfSlip" units="deg" type="float" elements="1"/>
    </data>
  </object>
</xml>
