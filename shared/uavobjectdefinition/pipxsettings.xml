<?xml version="1.0"?>
<xml>
  <object name="PipXSettings" singleinstance="true" settings="true">
    <meta>
      <description>PipXtreme configurations options.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="PairID" units="" type="uint32" elements="1" defaultvalue="0"/>
      <data name="TelemetryConfig" units="function" type="enum" elements="1" options="Disabled,Serial,UAVTalk,GCS,Debug" defaultvalue="UAVTalk"/>
      <data name="TelemetrySpeed" units="bps" type="enum" elements="1" options="2400,4800,9600,19200,38400,57600,115200" defaultvalue="57600"/>
      <data name="FlexiConfig" units="function" type="enum" elements="1" options="Disabled,Serial,UAVTalk,GCS,PPM_In,PPM_Out,RSSI,Debug" defaultvalue="Disabled"/>
      <data name="FlexiSpeed" units="bps" type="enum" elements="1" options="2400,4800,9600,19200,38400,57600,115200" defaultvalue="57600"/>
      <data name="VCPConfig" units="function" type="enum" elements="1" options="Disabled,Serial,Debug" defaultvalue="Disabled"/>
      <data name="VCPSpeed" units="bps" type="enum" elements="1" options="2400,4800,9600,19200,38400,57600,115200" defaultvalue="57600"/>
      <data name="RFSpeed" units="bps" type="enum" elements="1" options="2400,4800,9600,19200,38400,57600,115200" defaultvalue="115200"/>
      <data name="MaxRFPower" units="mW" type="enum" elements="1" options="1.25,1.6,3.16,6.3,12.6,25,50,100" defaultvalue="1.25"/>
      <data name="SendTimeout" units="ms" type="uint16" elements="1" defaultvalue="50"/>
      <data name="MinPacketSize" units="bytes" type="uint8" elements="1" defaultvalue="50"/>
      <data name="FrequencyCalibration" units="" type="uint8" elements="1" defaultvalue="127"/>
      <data name="Frequency" units="" type="uint32" elements="1" defaultvalue="434000000"/>
      <data name="AESKey" units="" type="uint8" elements="32" defaultvalue="0123456789abcdef0123456789abcdef"/>
    </data>
  </object>
</xml>
