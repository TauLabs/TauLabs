<?xml version="1.0"?>
<xml>
  <object name="AirspeedSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the @ref BaroAirspeed module used on CopterControl or Revolution</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="GPSSamplePeriod_ms" units="ms" type="uint8" elements="1" defaultvalue="100"/>
      <data name="ZeroPoint" units="raw" type="uint16" elements="1" defaultvalue="0"/>
      <data name="Scale" units="raw" type="float" elements="1" defaultvalue="1.0"/>
      <data name="AirspeedSensorType" units="" type="enum" elements="1" options="EagleTreeAirspeedV3,DIYDronesMPXV5004,DIYDronesMPXV7002,GPSOnly" defaultvalue="DIYDronesMPXV7002"/>
    </data>
  </object>
</xml>
