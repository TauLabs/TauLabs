	<field name="FlashOperation" units="" type="enum" elements="1" options="NONE,FAILED,COMPLETED,Save1,Save2,Save3,Save4,Save5,Load1,Load2,Load3,Load4,Load5" defaultvalue="NONE"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="onchange" period="0"/>
