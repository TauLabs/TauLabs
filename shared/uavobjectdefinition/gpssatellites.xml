<?xml version="1.0"?>
<xml>
  <object name="GPSSatellites" singleinstance="true" settings="false">
    <meta>
      <description>Contains information about the GPS satellites in view from @ref GPSModule.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="10000"/>
      <logging updatemode="periodic" period="30000"/>
    </meta>
    <data>
      <data name="SatsInView" units="" type="int8" elements="1"/>
      <data name="PRN" units="" type="int8" elements="16"/>
      <data name="Elevation" units="degrees" type="float" elements="16"/>
      <data name="Azimuth" units="degrees" type="float" elements="16"/>
      <data name="SNR" units="" type="int8" elements="16"/>
    </data>
  </object>
</xml>
