<?xml version="1.0"?>
<xml>
  <object name="MixerSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the @ref ActuatorModule that controls the channel assignments for the mixer based on AircraftType</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="MaxAccel" units="units/sec" type="float" elements="1" defaultvalue="1000"/>
      <data name="FeedForward" units="" type="float" elements="1" defaultvalue="0"/>
      <data name="AccelTime" units="ms" type="float" elements="1" defaultvalue="0"/>
      <data name="DecelTime" units="ms" type="float" elements="1" defaultvalue="0"/>
      <data name="ThrottleCurve1" units="percent" type="uint8" elementnames="0,25,50,75,100" defaultvalue="0,63,127,191,255"/>
      <data name="Curve2Source" units="" type="enum" elements="1" defaultvalue="Throttle" options="Throttle,Roll,Pitch,Yaw,Collective,Accessory0,Accessory1,Accessory2,Accessory3,Accessory4,Accessory5" />
      <data name="ThrottleCurve2" units="percent" type="uint8" elementnames="0,25,50,75,100" defaultvalue="0,63,127,191,255"/>
      <data name="Mixer1Type" units="" type="enum" elements="1" defaultvalue="Disabled" options="Disabled,Motor,Servo,CameraRoll,CameraPitch,CameraYaw,Accessory0,Accessory1,Accessory2,Accessory3,Accessory4,Accessory5"/>
       
      <data name="Mixer1Vector" units="" type="int8" defaultvalue="0" elementnames="ThrottleCurve1,ThrottleCurve2,Roll,Pitch,Yaw" />
      <data name="Mixer2Type" cloneof="Mixer1Type"/>
      <data name="Mixer2Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer3Type" cloneof="Mixer1Type"/>
      <data name="Mixer3Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer4Type" cloneof="Mixer1Type"/>
      <data name="Mixer4Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer5Type" cloneof="Mixer1Type"/>
      <data name="Mixer5Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer6Type" cloneof="Mixer1Type"/>
      <data name="Mixer6Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer7Type" cloneof="Mixer1Type"/>
      <data name="Mixer7Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer8Type" cloneof="Mixer1Type"/>
      <data name="Mixer8Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer9Type" cloneof="Mixer1Type"/>
      <data name="Mixer9Vector" cloneof="Mixer1Vector"/>
      <data name="Mixer10Type" cloneof="Mixer1Type"/>
      <data name="Mixer10Vector" cloneof="Mixer1Vector"/>
    </data>
  </object>
</xml>
