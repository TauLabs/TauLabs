<?xml version="1.0"?>
<xml>
  <object name="FixedWingPathFollowerStatus" singleinstance="true" settings="false">
    <meta>
      <description>Object Storing Debugging Information on PID internals</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="50000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="Error" units="" type="float" elementnames="Bearing,Speed,Accel,Power"/>
      <data name="ErrorInt" units="" type="float" elementnames="Bearing,Speed,Accel,Power"/>
      <data name="Command" units="" type="float" elementnames="Bearing,Speed,Accel,Power"/>
      <data name="Errors" units="" type="uint8" elementnames="Wind,Stallspeed,Lowspeed,Highspeed,Overspeed,Lowpower,Highpower,Pitchcontrol"/>
    </data>
  </object>
</xml>
