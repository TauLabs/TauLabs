<?xml version="1.0"?>
<xml>
  <object name="FlightPlanControl" singleinstance="true" settings="false">
    <meta>
      <description>Control the flight plan script</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="manual" period="0"/>
      <telemetryflight acked="true" updatemode="manual" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="Command" units="" type="enum" options="Start,Stop,Kill" defaultvalue="Start" elements="1"/>
    </data>
  </object>
</xml>
