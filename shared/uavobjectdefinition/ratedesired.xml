<?xml version="1.0"?>
<xml>
  <object name="RateDesired" singleinstance="true" settings="false">
    <meta>
      <description>Status for the matrix mixer showing the output of each mixer after all scaling</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="Roll" units="deg/s" type="float" elements="1"/>
      <data name="Pitch" units="deg/s" type="float" elements="1"/>
      <data name="Yaw" units="deg/s" type="float" elements="1"/>
    </data>
  </object>
</xml>
