<?xml version="1.0"?>
<xml>
  <object name="ActuatorCommand" singleinstance="true" settings="false">
    <data>
      <data name="Channel" units="us" type="int16" elements="10"/>
      <data name="UpdateTime" units="ms" type="uint8" elements="1"/>
      <data name="MaxUpdateTime" units="ms" type="uint16" elements="1"/>
      <data name="NumFailedUpdates" units="" type="uint8" elements="1"/>
    </data>
    <meta>
      <description>Contains the pulse duration sent to each of the channels.  Set by @ref ActuatorModule</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
  </object>
</xml>
