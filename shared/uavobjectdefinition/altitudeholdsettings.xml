<?xml version="1.0"?>
<xml>
  <object name="AltitudeHoldSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the @ref AltitudeHold module</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="Kp" units="throttle/m" type="float" elements="1" defaultvalue="0.03"/>
      <data name="Ki" units="throttle/m" type="float" elements="1" defaultvalue="0.00"/>
      <data name="Kd" units="throttle/m" type="float" elements="1" defaultvalue="0.03"/>
      <data name="Ka" units="throttle/(m/s^2)" type="float" elements="1" defaultvalue="0.005"/>
      <data name="PressureNoise" units="m" type="float" elements="1" defaultvalue="0.4"/>
      <data name="AccelNoise" units="m/s^2" type="float" elements="1" defaultvalue="5"/>
      <data name="AccelDrift" units="m/s^2" type="float" elements="1" defaultvalue="0.001"/>
    </data>
  </object>
</xml>
