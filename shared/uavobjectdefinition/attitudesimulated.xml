<?xml version="1.0"?>
<xml>
  <object name="AttitudeSimulated" singleinstance="true" settings="false">
    <meta>
      <description>The simulated Attitude estimation from @ref Sensors.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="100"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="q1" units="" type="float" elements="1"/>
      <data name="q2" units="" type="float" elements="1"/>
      <data name="q3" units="" type="float" elements="1"/>
      <data name="q4" units="" type="float" elements="1"/>
      <data name="Roll" units="degrees" type="float" elements="1"/>
      <data name="Pitch" units="degrees" type="float" elements="1"/>
      <data name="Yaw" units="degrees" type="float" elements="1"/>
      <data name="Velocity" units="m/s" type="float" elementnames="North, East, Down"/>
      <data name="Position" units="m" type="float" elementnames="North, East, Down"/>
    </data>
  </object>
</xml>
