<?xml version="1.0"?>
<xml>
  <object name="AttitudeActual" singleinstance="true" settings="false">
    <meta>
      <description>The updated Attitude estimation from @ref AHRSCommsModule.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="100"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="q1" units="" type="float" elements="1"/>
      <data name="q2" units="" type="float" elements="1"/>
      <data name="q3" units="" type="float" elements="1"/>
      <data name="q4" units="" type="float" elements="1"/>
      <data name="Roll" units="degrees" type="float" elements="1"/>
      <data name="Pitch" units="degrees" type="float" elements="1"/>
      <data name="Yaw" units="degrees" type="float" elements="1"/>
    </data>
  </object>
</xml>
