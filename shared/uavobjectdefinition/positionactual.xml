<?xml version="1.0"?>
<xml>
  <object name="PositionActual" singleinstance="true" settings="false">
    <meta>
      <description>Contains the current position relative to @ref HomeLocation</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="North" units="m" type="float" elements="1"/>
      <data name="East" units="m" type="float" elements="1"/>
      <data name="Down" units="m" type="float" elements="1"/>
    </data>
  </object>
</xml>
