<?xml version="1.0"?>
<xml>
  <object name="PipXStatus" singleinstance="true" settings="false">
    <meta>
      <description>PipXtreme device status.</description>
      <access gcs="readonly" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="Description" units="" type="uint8" elements="40"/>
      <data name="CPUSerial" units="" type="uint8" elements="12"/>
      <data name="BoardRevision" units="" type="uint16" elements="1"/>
      <data name="BoardType" units="" type="uint8" elements="1"/>
      <data name="MinFrequency" units="Hz" type="uint32" elements="1" defaultvalue="0"/>
      <data name="MaxFrequency" units="Hz" type="uint32" elements="1" defaultvalue="0"/>
      <data name="FrequencyBand" units="" type="uint8" elements="1" defaultvalue="0"/>
      <data name="FrequencyStepSize" units="" type="float" elements="1" defaultvalue="0"/>
      <data name="DeviceID" units="" type="uint32" elements="1" defaultvalue="0"/>
      <data name="Retries" units="" type="uint16" elements="1" defaultvalue="0"/>
      <data name="UAVTalkErrors" units="" type="uint16" elements="1" defaultvalue="0"/>
      <data name="Dropped" units="" type="uint16" elements="1" defaultvalue="0"/>
      <data name="Resets" units="" type="uint16" elements="1" defaultvalue="0"/>
      <data name="TXRate" units="Bps" type="uint16" elements="1" defaultvalue="0"/>
      <data name="RXRate" units="Bps" type="uint16" elements="1" defaultvalue="0"/>
      <data name="RSSI" units="dBm" type="int8" elements="1" defaultvalue="0"/>
      <data name="LinkQuality" units="" type="uint8" elements="1" defaultvalue="0"/>
      <data name="LinkState" units="function" type="enum" elements="1" options="Disconnected,Connecting,Connected" defaultvalue="Disconnected"/>
      <data name="PairIDs" units="" type="uint32" elements="4" defaultvalue="0"/>
      <data name="PairSignalStrengths" units="dBm" type="int8" elements="4" defaultvalue="-127"/>
    </data>
  </object>
</xml>
