<?xml version="1.0"?>
<xml>
  <object name="GPSPosition" singleinstance="true" settings="false">
    <meta>
      <description>Raw GPS data from @ref GPSModule.  Should only be used by @ref AHRSCommsModule.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="2000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="Status" units="" type="enum" elements="1" options="NoGPS,NoFix,Fix2D,Fix3D"/>
      <data name="Latitude" units="degrees x 10^-7" type="int32" elements="1"/>
      <data name="Longitude" units="degrees x 10^-7" type="int32" elements="1"/>
      <data name="Altitude" units="meters" type="float" elements="1"/>
      <data name="GeoidSeparation" units="meters" type="float" elements="1"/>
      <data name="Heading" units="degrees" type="float" elements="1"/>
      <data name="Groundspeed" units="m/s" type="float" elements="1"/>
      <data name="Satellites" units="" type="int8" elements="1"/>
      <data name="PDOP" units="" type="float" elements="1"/>
      <data name="HDOP" units="" type="float" elements="1"/>
      <data name="VDOP" units="" type="float" elements="1"/>
    </data>
  </object>
</xml>
