<?xml version="1.0"?>
<xml>
  <object name="InertialSensorSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the @ref Attitude module</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="AccelBias" units="m/s^2" type="float" elementnames="X,Y,Z" defaultvalue="0"/>
      <data name="AccelScale" units="[-]" type="float" elementnames="X,Y,Z" defaultvalue="1"/>
      <data name="InitialGyroBias" units="deg/s" type="float" elementnames="X,Y,Z" defaultvalue="0"/>
      <data name="GyroScale" units="-" type="float" elementnames="X,Y,Z" defaultvalue="1"/>
      <data name="GyroTempCoeff" units="(deg/s)/deg" type="float" elementnames="X,Y,Z" defaultvalue="1"/>
    </data>
  </object>
</xml>
