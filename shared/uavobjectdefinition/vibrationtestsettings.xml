<xml>
    <object name="VibrationTestSettings" singleinstance="true" settings="true">
        <description>Settings for the @ref VibrationTest Module</description>
        <field name="SampleRate" units="ms" type="uint16" elements="1" defaultvalue="20"/>
        <field name="TestPhase" units="" type="enum" elements="1" options="Off,Measuring,Computed" defaultvalue="Off"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="onchange" period="0"/>
        <telemetryflight acked="true" updatemode="onchange" period="0"/>
        <logging updatemode="manual" period="0"/>
	</object>
</xml>
