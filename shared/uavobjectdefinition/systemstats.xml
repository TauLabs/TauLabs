<?xml version="1.0"?>
<xml>
  <object name="SystemStats" singleinstance="true" settings="false">
    <meta>
      <description>CPU and memory usage from OpenPilot computer. </description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="FlightTime" units="ms" type="uint32" elements="1"/>
      <data name="HeapRemaining" units="bytes" type="uint16" elements="1"/>
      <data name="IRQStackRemaining" units="bytes" type="uint16" elements="1"/>
      <data name="CPULoad" units="%" type="uint8" elements="1"/>
      <data name="CPUTemp" units="C" type="int8" elements="1"/>
      <data name="EventSystemWarningID" units="uavoid" type="uint32" elements="1"/>
      <data name="ObjectManagerCallbackID" units="uavoid" type="uint32" elements="1"/>
      <data name="ObjectManagerQueueID" units="uavoid" type="uint32" elements="1"/>
    </data>
  </object>
</xml>
