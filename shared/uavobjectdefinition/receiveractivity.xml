<?xml version="1.0"?>
<xml>
  <object name="ReceiverActivity" singleinstance="true" settings="false">
    <meta>
      <description>Monitors which receiver channels have been active within the last second.</description>
      <access gcs="readonly" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="ActiveGroup" units="Channel Group" type="enum" elements="1" options="PWM,PPM,DSM (MainPort),DSM (FlexiPort),S.Bus,GCS,None" defaultvalue="None"/>
      <data name="ActiveChannel" units="channel" type="uint8" elements="1" defaultvalue="255"/>
    </data>
  </object>
</xml>
