<?xml version="1.0"?>
<xml>
  <object name="INSSettings" singleinstance="true" settings="true">
    <meta>
      <description>Settings for the INS to control the algorithm and what is updated</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <!-- Sensor noises -->
      <data name="accel_var" units="(m/s)^2" type="float" elementnames="X,Y,Z" defaultvalue="0.01"/>
      <data name="gyro_var" units="(deg/s)^2" type="float" elementnames="X,Y,Z" defaultvalue="0.01"/>
      <data name="mag_var" units="mGau^2" type="float" elementnames="X,Y,Z" defaultvalue="0.01,0.01,10"/>
      <data name="gps_var" units="m^2" type="float" elementnames="Pos,Vel" defaultvalue="1,1"/>
      <data name="baro_var" units="m^2" type="float" elements="1" defaultvalue="1"/>
      <!-- These settings are related to how the sensors are post processed -->
      <data name="MagBiasNullingRate" units="" type="float" elements="1" defaultvalue="0"/>
    </data>
  </object>
</xml>
