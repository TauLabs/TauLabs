<?xml version="1.0"?>
<xml>
  <object name="FaultSettings" singleinstance="true" settings="true">
    <meta>
      <description>Allows testers to simulate various fault scenarios.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="ActivateFault" units="fault" type="enum" elements="1" options="NoFault,ModuleInitAssert,InitOutOfMemory,InitBusError,RunawayTask,TaskOutOfMemory" defaultvalue="NoFault"/>
    </data>
  </object>
</xml>
