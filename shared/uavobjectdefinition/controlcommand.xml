<xml>
	<object name="ControlCommand" singleinstance="true" settings="false">
		<description>The output from the @ref ControlCommand module.</description>
		<field name="Throttle" units="%/100" type="float" elements="1"/>
		<field name="Roll" units="%/100" type="float" elements="1"/>
		<field name="Pitch" units="%/100" type="float" elements="1"/>
		<field name="Yaw" units="%/100" type="float" elements="1"/>
		<field name="Collective" units="%/100" type="float" elements="1"/>
		<field name="Flaps" units="%" type="int8" elements="1"/>
		<field name="Spoilers" units="%" type="int8" elements="1"/>
		<field name="LandingGear" units="%" type="int8" elements="1"/>
		<field name="GearShift" units="%" type="int8" elements="1"/>
		<field name="Lights" units="%" type="uint8" elements="1"/>

		<access gcs="readwrite" flight="readwrite"/>
		<telemetrygcs acked="false" updatemode="manual" period="0"/>
		<telemetryflight acked="false" updatemode="periodic" period="2000"/>
		<logging updatemode="manual" period="0"/>
	</object>
</xml>
