<?xml version="1.0"?>
<xml>
  <object name="OveroSyncStats" singleinstance="true" settings="false">
    <meta>
      <description>Maintains statistics on transfer rate to and from over</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="Connected" units="" type="enum" options="False,True" elements="1" default="False"/>
      <data name="Send" units="B/s" type="uint32" elements="1"/>
      <data name="Received" units="B/s" type="uint32" elements="1"/>
      <data name="FramesyncErrors" units="count" type="uint32" elements="1"/>
      <data name="UnderrunErrors" units="count" type="uint32" elements="1"/>
      <data name="DroppedUpdates" units="" type="uint32" elements="1"/>
      <data name="Packets" units="" type="uint32" elements="1"/>
    </data>
  </object>
</xml>
