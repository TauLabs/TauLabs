<xml>
    <object name="WindVelocityActual" singleinstance="true" settings="false">
        <description>Stores 3D wind speed estimation.</description>
        <field name="North" units="m/s" type="float" elements="1"/>
        <field name="East" units="m/s" type="float" elements="1"/>
        <field name="Down" units="m/s" type="float" elements="1"/>
        
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="periodic" period="2000"/>
        <logging updatemode="periodic" period="1000"/>
    </object>
</xml>
