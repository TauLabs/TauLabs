<?xml version="1.0"?>
<xml>
  <object name="ActuatorDesired" singleinstance="true" settings="false">
    <meta>
      <description>Desired raw, pitch and yaw actuator settings.  Comes from either @ref StabilizationModule or @ref ManualControlModule depending on FlightMode.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="Roll" units="% / 100" type="float" elements="1"/>
      <data name="Pitch" units="% / 100" type="float" elements="1"/>
      <data name="Yaw" units="% / 100" type="float" elements="1"/>
      <data name="Throttle" units="% / 100" type="float" elements="1"/>
      <data name="UpdateTime" units="ms" type="float" elements="1"/>
      <data name="NumLongUpdates" units="ms" type="float" elements="1"/>
    </data>
  </object>
</xml>
