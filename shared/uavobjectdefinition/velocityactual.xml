<?xml version="1.0"?>
<xml>
  <object name="VelocityActual" singleinstance="true" settings="false">
    <meta>
      <description>Updated by @ref AHRSCommsModule and used within @ref GuidanceModule for velocity control</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="periodic" period="1000"/>
    </meta>
    <data>
      <data name="North" units="m/s" type="float" elements="1"/>
      <data name="East" units="m/s" type="float" elements="1"/>
      <data name="Down" units="m/s" type="float" elements="1"/>
    </data>
  </object>
</xml>
