<?xml version="1.0"?>
<xml>
  <object name="GPSTime" singleinstance="true" settings="false">
    <meta>
      <description>Contains the GPS time from @ref GPSModule.  Required to compute the world magnetic model correctly when setting the home location.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="10000"/>
      <logging updatemode="periodic" period="30000"/>
    </meta>
    <data>
      <data name="Month" units="" type="int8" elements="1"/>
      <data name="Day" units="" type="int8" elements="1"/>
      <data name="Year" units="" type="int16" elements="1"/>
      <data name="Hour" units="" type="int8" elements="1"/>
      <data name="Minute" units="" type="int8" elements="1"/>
      <data name="Second" units="" type="int8" elements="1"/>
    </data>
  </object>
</xml>
