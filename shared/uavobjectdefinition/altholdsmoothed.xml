<?xml version="1.0"?>
<xml>
  <object name="AltHoldSmoothed" singleinstance="true" settings="false">
    <meta>
      <description>The output on the kalman filter on altitude.</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="false" updatemode="manual" period="0"/>
      <telemetryflight acked="false" updatemode="periodic" period="1000"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="Altitude" units="m" type="float" elements="1"/>
      <data name="Velocity" units="m/s" type="float" elements="1"/>
      <data name="Accel" units="m/s^2" type="float" elements="1"/>
    </data>
  </object>
</xml>
