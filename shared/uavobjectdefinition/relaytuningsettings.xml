<?xml version="1.0"?>
<xml>
  <object name="RelayTuningSettings" singleinstance="true" settings="true">
    <meta>
      <description>Setting to run a relay tuning algorithm</description>
      <access gcs="readwrite" flight="readwrite"/>
      <telemetrygcs acked="true" updatemode="onchange" period="0"/>
      <telemetryflight acked="true" updatemode="onchange" period="0"/>
      <logging updatemode="manual" period="0"/>
    </meta>
    <data>
      <data name="RateGain" units="" type="float" elements="1" defaultvalue="0.3333"/>
      <data name="AttitudeGain" units="" type="float" elements="1" defaultvalue="0.2"/>
      <data name="Amplitude" units="" type="float" elements="1" defaultvalue="0.25"/>
      <data name="HysteresisThresh" units="deg/s" type="uint8" elements="1" defaultvalue="5"/>
      <data name="Mode" units="" type="enum" elements="1" options="Rate,Attitude" defaultvalue="Attitude"/>
      <data name="Behavior" units="" type="enum" elements="1" options="Measure,Compute,Save" defaultvalue="Compute"/>
    </data>
  </object>
</xml>
